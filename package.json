{
  "name": "italia-backoffice",
  "version": "3.5.2",
  "engines": {
    "node": "^8.0.0",
    "yarn": "*"
  },
  "scripts": {
    "dev": "parcel index.html",
    "dev:proxy": "node api-proxy.js",
    "type-check": "tsc --noEmit",
    "distclean": "shx rm -rf dist",
    "postbuild": "shx cp CNAME 404.html web.config dist/",
    "deploy": "gh-pages -d dist -m 'Update GitHub Pages [ci skip]'",
    "lint": "tslint -p . -c tslint.json -t verbose",
    "commons-definitions:clean": "shx rm -rf generated/definitions/commons",
    "commons-definitions:mkdir": "shx mkdir -p generated/definitions/commons",
    "commons-definitions:generate": "gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/io-functions-commons/v22.0.1/openapi/index.yaml --out-dir ./generated/definitions/commons",
    "api-definitions:clean": "shx rm -rf generated/definitions/api",
    "api-definitions:mkdir": "shx mkdir -p generated/definitions/api",
    "api-definitions:generate": "gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/io-functions-admin/master/openapi/index.yaml --out-dir generated/definitions/api --no-strict",
    "backend-definitions:clean": "shx rm -rf generated/definitions/backend",
    "backend-definitions:mkdir": "shx mkdir -p generated/definitions/backend",
    "backend-definitions:generate": "gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/io-developer-portal-backend/master/definitions.yaml --out-dir generated/definitions/backend --no-strict",
    "generate": "npm-run-all api-definitions:* backend-definitions:* commons-definitions:*",
    "prebuild": "npm-run-all distclean generate type-check",
    "parcel:build": "parcel build index.html --public-url / --no-minify ./env-config.js",
    "parcel:serve": "parcel serve -p 8080 index.html --public-url / --no-minify ./env-config.js",
    "build": "npm-run-all parcel:build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "preversion": "auto-changelog  --config .auto-changelog.json --unreleased --commit-limit false --stdout --template preview.hbs",
    "version": "auto-changelog -p --config .auto-changelog.json --unreleased && git add CHANGELOG.md"
  },
  "resolutions": {
    "italia-utils/italia-ts-commons/json-set-map": "1.1.0",
    "parcel-bundler/htmlnano": "0.2.5",
    "parcel-bundler/htmlnano/posthtml": "0.12.3",
    "parcel-bundler/posthtml": "0.11.6",
    "fp-ts": "1.17.4"
  },
  "dependencies": {
    "@pagopa/ts-commons": "9.3.0",
    "auto-changelog": "^2.2.1",
    "batch": "^0.6.1",
    "bootstrap-italia": "^0.9.6",
    "design-react-kit": "^2.4.0",
    "fp-ts": "1.17.4",
    "i18next": "^14.1.1",
    "io-ts": "1.8.5",
    "lodash": "^4.17.21",
    "moment": "^2.22.2",
    "msal": "^1.2.1",
    "papaparse": "^4.6.0",
    "pouchdb": "^7.0.0",
    "pouchdb-browser": "^7.0.0",
    "pouchdb-find": "^7.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-datepicker": "^1.6.0",
    "react-dom": "^16.5.0",
    "react-i18next": "^9.0.10",
    "react-icons": "^2.2.7",
    "react-markdown": "^3.6.0",
    "react-pouchdb": "^0.2.1",
    "react-router-dom": "^4.3.1",
    "react-text-mask": "^5.4.3",
    "reactstrap": "^8.7.1",
    "recompose": "^0.27.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/i18next": "^12.1.0",
    "@types/jest": "^23.3.3",
    "@types/lodash": "^4.14.123",
    "@types/papaparse": "^4.5.9",
    "@types/pouchdb": "^6.3.3",
    "@types/pouchdb-browser": "^6.1.3",
    "@types/pouchdb-find": "^6.3.4",
    "@types/prop-types": "^15.6.2",
    "@types/react": "^16.5.2",
    "@types/react-datepicker": "^1.6.0",
    "@types/react-dom": "^16.5.0",
    "@types/react-icons": "^2.2.7",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-text-mask": "^5.4.3",
    "@types/reactstrap": "^6.0.1",
    "@types/recompose": "^0.27.1",
    "@types/request": "^2.48.1",
    "@types/webfontloader": "^1.6.28",
    "cors": "^2.8.4",
    "danger": "^7.1.4",
    "danger-plugin-digitalcitizenship": "^0.3.1",
    "express": "^4.16.3",
    "gh-pages": "^2.0.1",
    "io-functions-commons": "^14.6.0",
    "italia-tslint-rules": "^1.1.0",
    "italia-utils": "^4.0.0",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.10.2",
    "prettier": "^1.17.0",
    "request": "^2.88.0",
    "shx": "^0.3.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.16.0",
    "typescript": "3.4.4"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "tsx",
      "ts",
      "js",
      "node"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.ts"
    ],
    "moduleNameMapper": {
      ".*css$": "<rootDir>/src/components/input/__tests__/mock.css"
    }
  },
  "snyk": true
}
