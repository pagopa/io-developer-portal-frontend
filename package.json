{
  "name": "italia-backoffice",
  "version": "1.0.3",
  "engines": {
    "node": "^8.0.0",
    "yarn": "*"
  },
  "scripts": {
    "dev": "parcel index.html",
    "dev:proxy": "node api-proxy.js",
    "type-check": "tsc --noEmit",
    "prebuild": "yarn generate:definitions && yarn type-check",
    "build": "rimraf dist && parcel build index.html --public-url /",
    "postbuild": "shx cp CNAME 404.html web.config dist/",
    "deploy": "gh-pages -d dist -m 'Update GitHub Pages [ci skip]'",
    "lint": "tslint -p . -c tslint.json -t verbose",
    "generate:api-definitions": "rimraf generated/definitions/api && mkdir -p generated/definitions/api && gen-api-models --api-spec https://raw.githubusercontent.com/teamdigitale/digital-citizenship-functions/v0.64.0/api/admin_api.yaml --out-dir generated/definitions/api --no-strict",
    "generate:backend-definitions": "rimraf generated/definitions/backend && mkdir -p generated/definitions/backend && gen-api-models --api-spec https://raw.githubusercontent.com/teamdigitale/digital-citizenship-onboarding/v0.2.0/definitions.yaml --out-dir generated/definitions/backend --no-strict",
    "generate:definitions": "yarn generate:api-definitions && yarn generate:backend-definitions",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "batch": "^0.6.1",
    "bootstrap-italia": "^0.9.6",
    "design-react-kit": "^2.4.0",
    "i18next": "^14.1.1",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "msal": "^0.2.3",
    "papaparse": "^4.6.0",
    "pouchdb": "^7.0.0",
    "pouchdb-browser": "^7.0.0",
    "pouchdb-find": "^7.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-datepicker": "^1.6.0",
    "react-dom": "^16.5.0",
    "react-i18next": "^9.0.10",
    "react-icons": "^2.2.7",
    "react-markdown": "^3.6.0",
    "react-pouchdb": "^0.2.1",
    "react-router-dom": "^4.3.1",
    "react-text-mask": "^5.4.3",
    "recompose": "^0.27.1",
    "webfontloader": "^1.6.28",
    "snyk": "^1.226.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/i18next": "^12.1.0",
    "@types/lodash": "^4.14.123",
    "@types/papaparse": "^4.5.9",
    "@types/pouchdb": "^6.3.3",
    "@types/pouchdb-browser": "^6.1.3",
    "@types/pouchdb-find": "^6.3.4",
    "@types/prop-types": "^15.6.2",
    "@types/react": "^16.5.2",
    "@types/react-datepicker": "^1.6.0",
    "@types/react-dom": "^16.5.0",
    "@types/react-icons": "^2.2.7",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-text-mask": "^5.4.3",
    "@types/reactstrap": "^6.0.1",
    "@types/recompose": "^0.27.1",
    "@types/request": "^2.48.1",
    "@types/webfontloader": "^1.6.28",
    "cors": "^2.8.4",
    "danger": "^7.1.4",
    "danger-plugin-digitalcitizenship": "^0.3.1",
    "express": "^4.16.3",
    "gh-pages": "^2.0.1",
    "italia-tslint-rules": "^1.1.0",
    "italia-utils": "^4.0.0",
    "parcel-bundler": "^1.10.2",
    "prettier": "^1.17.0",
    "request": "^2.88.0",
    "rimraf": "^2.6.3",
    "shx": "^0.3.2",
    "tslint": "^5.16.0",
    "typescript": "^3.4.2"
  },
  "snyk": true
}
